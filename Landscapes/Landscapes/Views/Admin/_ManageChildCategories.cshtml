@model IEnumerable<Landscapes.Models.ChildCategory>

@*<table id="tblSub">*@
<link href="~/Scripts/DataTable/bootstrap.min.css" rel="stylesheet" />
<link href="~/Scripts/DataTable/dataTables.bootstrap.min.css" rel="stylesheet" />
<table id="tblSub" class="table table-striped table-bordered" cellspacing="0" width="100%">

    <thead>
        <tr>
            <th>Image</th>
            <th>ChildCategory</th>
            <th>Delete</th>
          @*  <th>Edit</th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        { 
            <tr>
                <td>
                 <img style="max-width:100px;height:100px" src="@item.Image"/></td>
                <td>@item.SubcategoryName</td>
                <td><a style="cursor:pointer;" onclick="DeleteSubactegory(@item.ChildCategory_Id)">Delete</a></td>
             @*   <td><a onclick="EditSubcategory(@item.ChildCategory_Id)">Edit</a></td>*@
            </tr>
        }
    </tbody>
</table>

<script src="~/Scripts/DataTable/jquery-1.12.3.js"></script>
<script src="~/Scripts/DataTable/jquery.dataTables.min.js"></script>
<script src="~/Scripts/DataTable/dataTables.bootstrap.min.js"></script>
<script>
    $(document).ready(function () {
        $("#tblSub").DataTable();
    });
</script>
