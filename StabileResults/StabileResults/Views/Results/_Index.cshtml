@model IEnumerable<StabileResults.Models.Result>

<link href="~/Scripts/DataTable/bootstrap.min.css" rel="stylesheet" />
<link href="~/Scripts/DataTable/dataTables.bootstrap.min.css" rel="stylesheet" />
<table id="tblResult" class="table table-striped table-bordered" cellspacing="0" width="100%">

    <thead>
        <tr>
            <th>Added Date</th>
            <th>Result Heading</th>
            <th>Result</th>
            <th>UserName</th>
            <th>Delete</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        { 
            <tr>
                <td>@item.Date</td>
                <td>@item.Result_Heading</td>
                <td style="width: 280px">@item.Result_Text</td>
                <td>@item.UserName</td>

                @if (Convert.ToInt32(Session["UserId"]) == item.tblUserId || Convert.ToInt32(Session["tblUserRoleId"]) == 1)
                {
                    <td><a style="cursor:pointer" onclick="DeleteResult(@item.tblResultId)">Delete</a></td>
                    <td><a style="cursor:pointer" onclick="EditResult(@item.tblResultId)">Edit</a></td>
                }
                else
                {
                    <td></td>
                    <td></td>
                }
            </tr>
        }
    </tbody>
</table>
<script src="~/Scripts/DataTable/jquery-1.12.3.js"></script>
<script src="~/Scripts/DataTable/jquery.dataTables.min.js"></script>
<script src="~/Scripts/DataTable/dataTables.bootstrap.min.js"></script>
<script>
    $(document).ready(function () {
        $('#tblResult').DataTable();
    });
</script>
