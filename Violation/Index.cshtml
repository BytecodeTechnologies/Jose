        <script type="text/javascript">
            $(document).ready(function () {
                $(".fancybox").fancybox();
            });

            $("#SubmitEnqueryData").click(function () {
                captcharesponse = grecaptcha.getResponse();
                Name = $("#inputName").val();
                Email = $('#inputEmail').val();
                Phoneno = $('#inputPhone').val();
                Message = $('#inputMsg').val();
                RequestFrom = "Home";
                if (Name == "") {
                    alert('Please type Your Name')
                    return;
                }
                if (Email == "") {
                    alert('Please Type Your Email Id')
                    return;
                }
                if (Phoneno == "") {
                    alert('Please Type Your Contact Number')
                    return;
                }
                if (Message == "") {
                    alert('Please Type Your Message')
                    return;
                }
                if (captcharesponse == "") {
                    alert('Please verify Capcha')
                    return;
                }

                if (!ValidateEmail(Email)) {
                    $('.loadercont').fadeOut();
                    $('.loadercont').fadeOut();
                    alert('not a valid e-mail address');
                    return;
                }

                var data = { Name: Name, Email: Email, phoneno: Phoneno, Messeage: Message, RequestFrom: RequestFrom };
                $.ajax({
                    type: "POST",
                    url: "/Home/SendEnqueryEmail",
                    data: data,
                    dataType: "json",
                    success: function (data) {
                        alert(data)
                    },
                    error: function (data) {
                        alert(data)
                    }
                });

            });

            function ValidateEmail(email) {
                // Validate email format
                var expr = /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
                return expr.test(email);
            };
            $("#inputPhone").keypress(function (e) {

                if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                    //display error message
                    return false;
                }
            });



        </script>
